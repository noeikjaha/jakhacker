n1=int(input("first 정수를 입력하시오>>"))
n2=int(input("sd 정수를 입력하시오>>"))
n3=int(input("rd 정수를 입력하시오>>"))

if (n1>=n2) and (n1>n3):
  largest = n1
elif (n2 >= n1) and (n2>=n3):
  largest = n2
else:
  largest = n3

print("가장 큰 숫자는 , " ,largest ," 입니다")

A= int(input("강아지의 나이를 입력하시오: "))

if A<0 :
  print("음수는  안된다")
  exit()
elif A<=2:
  B =A*10

else:
  B= 20+(A-2)*4

